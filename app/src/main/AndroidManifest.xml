<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.kuduvai.cachii.kuduvai">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <!-- Internet Permissions -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OTP">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".StockUpdate" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".CustomerList" />
        <activity android:name=".NewCustomer" />
        <activity android:name=".CustomerDisplay" />
        <activity android:name=".NewPurchase" />
        <activity android:name=".PurchaseList" />
        <activity android:name=".NewBooking" />
        <activity android:name=".BookingListDel" />
        <activity android:name=".DeliveryStatus" />
        <activity android:name=".ContactUs"></activity>
        <activity android:name=".AmountListDel"/>
    </application>

</manifest>